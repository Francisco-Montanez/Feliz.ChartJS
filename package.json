        "core-js": "^3.3.2",
        "http-proxy-middleware": "^3.0.3",
        "i18next": "^20.3.5",
        "open-sans-fonts": "^1.6.2",
        "react": "^18.2.0",
        "remotedev": "^0.2.0",
        "react-dom": "^18.2.0",
        "@tailwindcss/vite" : "^4.0.3",
        "@tailwindcss/typography" : "^0.5.16",
        "tailwindcss": "^4.0.3",
        "cssnano": "^7.0.6",
        "autoprefixer": "^10.4.2",
